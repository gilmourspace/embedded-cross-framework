# Optional configuration to use particular components.
# The format is CONFIG_USE_${component}
#
# For example:
# set(CONFIG_USE_MBEDTLS true)
# set(CONFIG_USE_CAN true)
# set(CONFIG_USE_ETH true)
# set(CONFIG_USE_USB true)
# set(CONFIG_USE_SPI true)
# set(CONFIG_USE_I2C true)
# set(CONFIG_USE_UART true)
# set(CONFIG_USE_PWM true)
# set(CONFIG_USE_ADC true)
# set(CONFIG_USE_RTC true)
